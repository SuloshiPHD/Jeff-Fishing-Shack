<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.control.PasswordField?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="screensframework.EmailenqController">
   <children>
      <Label alignment="CENTER" layoutX="143.0" layoutY="-13.0" prefHeight="62.0" prefWidth="297.0" text="Email Enquiries" textFill="#71099e">
         <font>
            <Font name="Verdana Bold" size="27.0" />
         </font>
      </Label>
      <TextField fx:id="emailAdd" layoutX="80.0" layoutY="70.0" prefHeight="30.0" prefWidth="339.0" promptText="Enter Your Email Address Here">
         <font>
            <Font size="17.0" />
         </font>
      </TextField>
       <PasswordField fx:id="emailpw" layoutX="80.0" layoutY="119.0" prefHeight="38.0" prefWidth="360.0" promptText="Enter Your Password for your Email Address">
           <font>
               <Font size="17.0" />
           </font>
       </PasswordField>
      <TextField fx:id="enqtxt" alignment="TOP_LEFT" layoutX="80.0" layoutY="171.0" prefHeight="169.0" prefWidth="452.0" promptText="Comments and Enquiries">
         <font>
            <Font size="19.0" />
         </font>
      </TextField>
      <Button layoutX="481.0" layoutY="347.0" mnemonicParsing="false" onAction="#sendEmail" prefHeight="44.0" prefWidth="93.0" text="Send" textFill="#970606">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
